{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "image": "latest",
        "simulator": false
      },
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    },
    "production": {
      "autoIncrement": false,
      "ios": {
        "image": "latest",
        "distribution": "store"
      },
      "env": {
        "RCT_NEW_ARCH_ENABLED": "1"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {}
    }
  }
}
